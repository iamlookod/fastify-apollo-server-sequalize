{
  "name": "fastify-apollo-server-sequalize",
  "version": "1.0.0",
  "description": "Project template Fastify Apollo-Server Sequalize Postgres",
  "main": "src/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently -k -p \"[{name}]\" -n \"App,App\" -c \"yellow.bold,cyan.bold\" \"nodemon\"",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/app.js",
    "db:migrate": "ts-node node_modules/.bin/sequelize migrate",
    "db:migration:generate": "ts-node node_modules/.bin/sequelize migration:generate --name",
    "db:undo": "ts-node node_modules/.bin/sequelize migration:undo",
    "db:migration:undo:all": "ts-node node_modules/.bin/sequelize migration:undo:all",
    "db:seed": "ts-node node_modules/.bin/sequelize seed --name",
    "db:seed:all": "ts-node node_modules/.bin/sequelize seed:all",
    "db:seed:generate": "ts-node node_modules/.bin/sequelize seed:generate --name",
    "db:seed:undo": "ts-node node_modules/.bin/sequelize seed:undo"
  },
  "keywords": [
    "fastify",
    "apollo",
    "apollo-server",
    "sequalize",
    "postgres"
  ],
  "author": "iamlookod",
  "license": "ISC",
  "dependencies": {
    "apollo-server-core": "^3.0.0",
    "apollo-server-fastify": "^3.0.0",
    "dotenv": "^10.0.0",
    "fastify": "^3.19.1",
    "fastify-cors": "^6.0.2",
    "graphql": "^15.5.1",
    "graphql-tools": "^7.0.5",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.5",
    "sequelize-typescript": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.3.2",
    "@types/validator": "^13.6.3",
    "concurrently": "^6.2.0",
    "fastify-tsconfig": "^1.0.1",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
